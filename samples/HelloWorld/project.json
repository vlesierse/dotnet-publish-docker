{
  "name": "HelloWorld",
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "tools": {
    "dotnet-publish-docker": "1.0.0-alpha1"
  },
  "scripts": {
    "postpublish": "dotnet publish-docker --publish-folder %publish:OutputPath%"
  }
